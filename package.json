{
  "name": "duel-log-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @duel-log/web --filter @duel-log/api --parallel dev",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r typecheck",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "pnpm -r test",
    "test:watch": "pnpm --filter @duel-log/web test:watch",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "esbuild"]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.0",
    "hono": "^4.11.7",
    "lucide-react": "^0.562.0",
    "postgres": "^3.4.8",
    "zod": "^3.25.76"
  }
}
